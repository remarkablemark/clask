{
  "name": "express-template",
  "version": "0.0.0",
  "description": "An Express web application template.",
  "author": "Mark <mark@remarkablemark.org>",
  "private": true,
  "scripts": {
    "start": "npm run build && NODE_ENV=production node ./bin/www",
    "build": "webpack --config webpack/production.config.js",
    "dev": "npm-run-all --parallel development:*",
    "development:server": "NODE_ENV=development DEBUG=express-template:* nodemon ./bin/www",
    "development:wds": "webpack-dev-server --config webpack/development.config.js"
  },
  "main": "bin/www",
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/express-template"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/express-template/issues"
  },
  "keywords": [
    "express",
    "template"
  ],
  "dependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.9",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "debug": "2.3.3",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "morgan": "1.7.0",
    "nodemon": "1.11.0",
    "nunjucks": "3.0.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "serve-favicon": "2.3.2",
    "socket.io": "1.7.1",
    "webpack": "1.14.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.2",
    "webpack-dev-server": "1.16.2"
  },
  "license": "MIT"
}
